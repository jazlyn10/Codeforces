#include <iostream>

using namespace std;

int main() {
    long long int n;
    cin >> n;
    int number = 0;

    while (n != 0) {
        if (n % 10 == 4 || n % 10 == 7)
            number++;
        n = n / 10;
    }
    int l = number;
    if (l == 4 || l == 7)
    cout << "YES";
    
    else cout << "NO";

    return 0;
}


